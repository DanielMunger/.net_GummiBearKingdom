@{
    ViewData["Title"] = "Products";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model List<GummiBearKingdom.Models.Product>
<h1>Products Carried by Gummi Bear Kingdom</h1>
<div class="row">
    <div class="col md3">Test Space</div>
    <div class="col" md9>
        @foreach(var product in Model)
        {
            <a href="/Products/Details/@product.ProductId">
                <div class="card">
                    <div class="card-content">
                        <h4>
                            @product.Name
                        </h4>
                    </div>
                </div>
            </a>

        }
    </div>
</div>